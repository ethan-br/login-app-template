{
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": false,
    "arrowParens": "always",
    "useTabs": false,
    "importOrder": ["^[./]"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
